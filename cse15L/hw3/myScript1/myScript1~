#!/bin/bash
# POSITIONAL PARAMETER HOMEWORK
# due 1/30/17 @ 11:59PM
# name:
# PID:
#
# usage example: ./script firstargument secondargument
#
# for this assignment, please replace the "_TODO_" with the
# appropriate answers. Please note that each of the answers
# will start with the $ that is already provided.


echo $0         # print the name of the script
echo $1         # print the first argument
echo $2         # print the second argument
echo $$         # print the shell pid
echo $?         # print the exit code
echo $#         # print the number of arguments
echo $LOGNAME         # print the login name
echo $*        # print all the arguments
